---
import { AuthorUniserve } from '@/data/Authors';
import { getImageById } from '@/utils/sharedImages';
import { getImage } from '@astrojs/image';
import UniserveConclusion from './UniserveConclusion.component'
    
const uniserveImageObject = getImageById("uniserve-welcome-modern")
const uniserveImageProcessed = await getImage({
    src: uniserveImageObject!.src,
    width: 400,
    aspectRatio: uniserveImageObject!.width / uniserveImageObject!.height,
    format: "webp",
    alt: ""
})

---

<UniserveConclusion 
    imageProcessed={uniserveImageProcessed}
    client:only
/>