---
import { getImageById }  from '@/data/sharedImages';
import { getImage } from 'astro:assets';
import UniserveConclusion from './UniserveConclusion.component'
    
const uniserveImageObject = getImageById("uniserve-welcome-modern")
const importedImage = await uniserveImageObject.src()
const importedImageInner = importedImage.default
const uniserveImageProcessed = await getImage({
    src: importedImageInner,
    width: 400,
    format: "webp",
    alt: ""
})

---

<UniserveConclusion 
    imageProcessed={uniserveImageProcessed}
    client:only
/>